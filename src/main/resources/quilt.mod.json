{
  "schema_version": 1,
  "quilt_loader": {
    "id": "${mod_id}",
    "version": "${mod_version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "icon": "${mod_id}.icon.png",
      "contributors": {
        "VanderCat": "Port",
        "Ocelot": "Maintainer",
        "Jackson": "Owner",
        "Farcr": "Art",
        "Echolite": "Art",
        "AstraZoey": "SoundDesign"
      },
      "license": "GPL-3.0-only"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "gg.moonflower.etched.core.quilt.EtchedFabric"
      ],
      "client_init": [
        "gg.moonflower.etched.core.quilt.EtchedFabricClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": "*"
      },
      {
        "id": "quilted_fabric_api",
        "versions": "*"
      }
    ],
    "group": "${archives_base_name}"
  },
  "mixins": [
    "etched.mixins.json",
    "fabric.mixins.json"
  ]
}